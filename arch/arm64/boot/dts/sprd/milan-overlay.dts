					220 221 222 223 224 225 226 227 228 229
					230 231 232 233 234 235 236 237 238 239
					240 241 242 243 244 245 246 247 248 249
					250 251
				>;
				default-brightness-level = <33>;
			}; ***/

			flash-gpios {
				compatible = "sprd,flash-aw3641";
 				flash-ic = <3641>;
 				flash-torch-en-gpios = <&ap_gpio 89 0>;
 				flash-en-gpios = <&ap_gpio 32 0>;
 			};
			
			bat: battery {
				compatible = "simple-battery";
				charge-full-design-microamp-hours = <4802000>;
				charge-term-current-microamp = <240000>;
				constant-charge-voltage-max-microvolt = <4432000>;
				factory-internal-resistance-micro-ohms = <120000>;
				voltage-min-design-microvolt = <3455000>;
				fullbatt-voltage = <4370000>;
				fullbatt-current = <240000>;
				fullbatt-track-end-vol = <4370000>;
				fullbatt-track-end-cur = <260000>;
				first-calib-voltage = <3570000>;
				first-calib-capacity = <40>;

				ocv-capacity-celsius = <20>;
				ocv-capacity-table-0 = <4382000 100>, <4324000 95>, <4266000 90>,
							<4208000 85>, <4151000 80>, <4097000 75>,
							<4048000 70>, <4002000 65>, <3957000 60>,
							<3902000 55>, <3866000 50>, <3841000 45>,
							<3821000 40>, <3804000 35>, <3790000 30>,
							<3770000 25>, <3750000 20>, <3726000 15>,
							<3698000 10>, <3684000 5>, <3400000 0>;
				voltage-temp-table = <1404000 650>, <1307000 700>, <1203000 750>,
							<1095000 800>, <986000 850>, <878000 900>,
							<775000 950>, <678000 1000>, <590000 1050>,
							<510000 1100>, <440000 1150>, <378000 1200>,
							<324000 1250>, <278000 1300>, <238000 1350>,
							<204000 1400>, <175000 1450>, <150000 1500>,
							<129000 1550>, <111000 1600>, <96000 1650>,
							<83000 1700>, <73000 1750>, <63000 1800>,
							<55000 1850>, <48000 1900>, <42000 1950>;
				capacity-temp-table = <45 100>, <25 100>, <10 97>, <0 88>, <(-10) 78>;
				resistance-temp-table = <45 83>, <25 100>, <10 178>, <0 880>, <(-10) 1012>;

				charge-sdp-current-microamp = <650000 500000>;
				charge-dcp-current-microamp = <2200000 2000000>;
				charge-cdp-current-microamp = <1600000 1500000>;
				charge-unknown-current-microamp = <2200000 2000000>;

				dcp-jeita-temp-table = <900 920 0 4432000 0 4180000>,
					<980 1000  950000  4432000 720000 4180000>,
					<1030 1050 2300000 4432000 1200000 4180000>,
					<1100 1120 2300000 4432000 2300000 4432000>,
					<1140 1160 2300000 4432000 2300000 4432000>,
					<1450 1430 2300000 4432000 2300000 4432000>,
					<1530 1510 1200000 4130000 1200000 4130000>;
				sdp-jeita-temp-table = <900 920 0 4432000 0 4180000>,
					<980 1000  650000 4432000 650000 4180000>,
					<1030 1050 650000 4432000 650000 4432000>,
					<1100 1120 650000 4432000 650000 4432000>,
					<1140 1160 650000 4432000 650000 4432000>,
					<1450 1430 650000 4432000 650000 4432000>,
					<1530 1510 650000 4130000 650000 4130000>;
				cdp-jeita-temp-table = <900 920 0 4432000 0 4180000>,
					<980 1000   950000  4432000 720000 4180000>,
					<1030 1050 1600000 4432000 1200000 4180000>,
					<1100 1120 1600000 4432000 1600000 4432000>,
					<1140 1160 1600000 4432000 1600000 4432000>,
					<1450 1430 1600000 4432000 1600000 4432000>,
					<1530 1510 1200000 4130000 1200000 4130000>;
				unknown-jeita-temp-table = <900 920 0 4432000 0 4180000>,
					<980 1000  950000  4432000 720000 4180000>,
					<1030 1050 2300000 4432000 1200000 4180000>,
					<1100 1120 2300000 4432000 2300000 4432000>,
					<1140 1160 2300000 4432000 2300000 4432000>,
					<1450 1430 2300000 4432000 2300000 4432000>,
					<1530 1510 1200000 4130000 1200000 4130000>;
				force-jeita-status = <2>;
			};

			charger-manager {
				status = "okay";
				compatible = "charger-manager";
				cm-name = "battery";
				cm-poll-mode = <2>;
				cm-poll-interval = <5000>;
				cm-battery-stat = <2>;

				cm-fullbatt-vchkdrop-ms = <30000>;
				cm-fullbatt-vchkdrop-volt = <100000>;
				cm-fullbatt-capacity = <100>;

				cm-num-chargers = <1>;
				cm-chargers = "sgm4151x_charger";
				cm-fuel-gauge = "sc27xx-fgu";

				/* in deci centigrade */
				cm-battery-cold = <200>;
				cm-battery-cold-in-minus;
				cm-battery-hot = <800>;
				cm-battery-temp-diff = <100>;

				/* Allow charging for 25.6hr */
				cm-charging-max = <92160000>;
				/* recovery charging after stop charging 45min */
				cm-discharging-max = <2700000>;

				/* the interval to feed charger watchdog */
				cm-wdt-interval = <60>;

				/* drop voltage in microVolts to allow shutdown */
				cm-shutdown-voltage = <3200000>;

				/* when 99% of the time is exceeded, it will be forced to 100% */
				cm-tickle-time-out = <580>;

				/* how much time to allow capacity change */
				cm-one-cap-time = <30>;

				/* when the safe charging voltage is exceeded, stop charging */
				cm-charge-voltage-max = <5800000>;
				/* drop voltage in microVolts to restart charging */
				cm-charge-voltage-drop = <350000>;

				monitored-battery = <&bat1 &bat2 &bat>;

				cm-thermal-zone = "board-thmzone";

			//	cm-cap-remap-table = <1 2 3>, <2 3 2>, <98 99 2>, <99 100 3>;

				io-channels = <&pmic_adc 3>;
				io-channel-names = "bat-id-vol";
				regulator@0 {
					cm-regulator-name = "vddgen0";
					cable@0 {
						cm-cable-name = "USB";
						extcon = <&extcon_gpio>;
					};
				};
			};

			tboard_thermistor:thermal-sensor {
				compatible = "generic-adc-thermal";
				#thermal-sensor-cells = <0>;
				io-channels = <&pmic_adc 1>;
				io-channel-names = "sensor-channel";
				temperature-lookup-table = < (-39742) 1131
							(-29777) 1110
							(-19480) 1074
							(-4679) 983
							10110 838
							24805 654
							44922 413
							54903 316
							64895 238
							74884 178
							84883 133
							94883 100
							104880 75
							114889 57
							124872 43 >;
			};

			trfpa_thermistor:rf-sensor {
				compatible = "generic-adc-thermal";
				#thermal-sensor-cells = <0>;
				io-channels = <&pmic_adc 2>;
				io-channel-names = "sensor-channel";
				temperature-lookup-table = < (-39742) 1131
							(-29777) 1110
							(-19480) 1074
							(-4679) 983
							10110 838
							24805 654
							44922 413
							54903 316
							64895 238
							74884 178
							84883 133
							94883 100
							104880 75
							114889 57
							124872 43 >;
			};
			
			tchg_thermistor:chg-sensor {
				compatible = "generic-adc-thermal";
				#thermal-sensor-cells = <0>;
				io-channels = <&pmic_adc 4>;
				io-channel-names = "sensor-channel";
				temperature-lookup-table = < (-39742) 1131
							(-29777) 1110
							(-19480) 1074
							(-4679) 983
							10110 838
							24805 654
							44922 413
							54903 316
							64895 238
							74884 178
							84883 133
							94883 100
							104880 75
							114889 57
							124872 43 >;
			};
			gpio-keys {
				compatible = "gpio-keys";
				pinctrl-names = "eic_dbc2";
				pinctrl-0 = <&eic_dbc2>;
				key-cut {
					label = "Cut Key";
					linux,code = <756>;
					gpios = <&ap_gpio 130 GPIO_ACTIVE_HIGH>;
					debounce-interval = <2>;
					wakeup-source;
				};
				key-volumedown {
					label = "Volume Down Key";
					linux,code = <KEY_VOLUMEDOWN>;
					gpios = <&eic_debounce 2 GPIO_ACTIVE_LOW>;
					debounce-interval = <20>;
					wakeup-source;
				};
				key-volumeup {
					label = "Volume Up Key";
					linux,code = <KEY_VOLUMEUP>;
					gpios = <&pmic_eic 10 GPIO_ACTIVE_HIGH>;
					debounce-interval = <2>;
					wakeup-source;
				};
				key-power {
					label = "Power Key";
					linux,code =<KEY_POWER>;
					gpios = <&pmic_eic 1 GPIO_ACTIVE_LOW>;
					debounce-interval = <2>;
					wakeup-source;
				};
			};
			sprd_uid {
				compatible = "sprd-uid";
				nvmem-cells = <&uid_start>, <&uid_end>;
				nvmem-cell-names = "uid_start", "uid_end";
			};
			/*Used to show hardware information for hq factory mode*/
 			hwinfo:HardwareInfo {
 				compatible = "huaqin,hardware_info";
 				sdcard-gpio = <&ap_gpio 78 0>;
 				status = "okay";
 			};
			
			/*Used to show hardware information for factory mode*/
			devices_devinfo:devices_devinfo {
				compatible = "devices,dev_info";
				status = "okay";
			};
			
			 uart0_log{
				compatible = "tinno,uart_log";
				pinctrl-names = "gpio_60_f0", "gpio_61_f0", "gpio_60_f3", "gpio_61_f3";
				pinctrl-0 = <&gpio_60_f0>;
				pinctrl-1 = <&gpio_61_f0>;
				pinctrl-2 = <&gpio_60_f3>;
				pinctrl-3 = <&gpio_61_f3>;
				status = "okay";
			};
			
		};
	};
};

&uart0 {

 status = "okay";
 

};
	
&chosen {
	
	bootargs_ext = "androidboot.hardware=milan androidboot.dtbo_idx=0";
};

&reserved_memory {
	#address-cells = <2>;
	#size-cells = <2>;

	logo_reserved: logobuffer@9e000000 {
		reg = <0x0 0x9e000000 0x0 0x007e9000>;
	};

	fd_reserved: faceid-mem@ffa70000{
		reg = <0x0 0xffa70000 0x0 0x00500000>;
	};
};

&dpu {
	sprd,logo-memory = <&logo_reserved>;
};


&ddr_dfs {
	sprd-scene = "boost", "lcdon", "lcdoff", "camlow",
		"camhigh", "camveryhigh", "top";
	sprd-freq = <933 400 233 400 622 933 933>;
};

&panel {
	/*** sprd,backlight = <&pwm_backlight>; ***/
	avdd-gpio = <&ap_gpio 4 GPIO_ACTIVE_HIGH>;
	avee-gpio = <&ap_gpio 5 GPIO_ACTIVE_HIGH>;
	reset-gpio = <&ap_gpio 50 GPIO_ACTIVE_HIGH>;
};

&thm_zone {
	bd_thmzone: board-thmzone {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-sensors = <&tboard_thermistor>;
	};
	rf_thmzone: rf-thmzone {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-sensors = <&trfpa_thermistor>;
	};
	chg_thmzone: chg-thmzone {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-sensors = <&tchg_thermistor>;
	};
};

&i2c0 {
	#address-cells = <1>;
	#size-cells = <0>;

	status = "okay";
	clock-frequency = <400000>;

	sensor_main: sensor-main@20 {
		compatible = "sprd,sensor-main";
		reg = <0x20>;
		clock-names = "clk_src","sensor_eb",
			"clk_96m","clk_76m8",
			"clk_48m","clk_26m";
		clocks = <&aon_clk CLK_SENSOR0>, <&mm_gate CLK_SENSOR0_EB>,
			<&pll CLK_TWPLL_96M>,<&pll CLK_TWPLL_76M8>,
			<&pll CLK_TWPLL_48M>,<&ext_26m>;
		vddio-supply = <&vddcamio>;
		vddcama-supply = <&vddcama>;
		vddcamd-supply = <&vddcamd>;
		vddcammot-supply = <&vddcammot>;
		sprd,phyid = <1>;
		csi = <&csi0>;
		reset-gpios = <&ap_gpio 44 0>;
		power-down-gpios = <&ap_gpio 46 0>;
		power-domains = <&mm_domain>;
	};
};

&i2c1 {
	#address-cells = <1>;
	#size-cells = <0>;

	status = "okay";
	clock-frequency = <400000>;

	sensor_sub: sensor-sub@20 {
		compatible = "sprd,sensor-sub";
		reg = <0x20>;
		clock-names = "clk_src","sensor_eb",
			"clk_96m","clk_76m8",
			"clk_48m","clk_26m";
		clocks = <&aon_clk CLK_SENSOR1>, <&mm_gate CLK_SENSOR1_EB>,
			<&pll CLK_TWPLL_96M>,<&pll CLK_TWPLL_76M8>,
			<&pll CLK_TWPLL_48M>,<&ext_26m>;
		vddio-supply = <&vddcamio>;
		vddcama-supply = <&vddcama>;
		vddcamd-supply = <&vddcamd>;
		vddcammot-supply = <&vddcammot>;
		sprd,phyid = <2>;
		csi = <&csi1>;
		reset-gpios = <&ap_gpio 41 0>;
		/*power-down-gpios = <&ap_gpio 40 0>;
		power-domains = <&mm_domain>;*/
	};

	sensor_main2: sensor-main2@6c {
		compatible = "sprd,sensor-main2";
		reg = <0x6C>;
		clock-names = "clk_src","sensor_eb",
			"clk_96m","clk_76m8",
			"clk_48m","clk_26m";
		clocks = <&aon_clk CLK_SENSOR1>, <&mm_gate CLK_SENSOR1_EB>,
			<&pll CLK_TWPLL_96M>,<&pll CLK_TWPLL_76M8>,
			<&pll CLK_TWPLL_48M>,<&ext_26m>;
		vddio-supply = <&vddcamio>;
		vddcama-supply = <&vddcama>;
		sprd,phyid = <3>;
		csi = <&csi1>;
		/*reset-gpios = <&ap_gpio 45 0>;*/
		power-down-gpios = <&ap_gpio 47 0>;
		power-domains = <&mm_domain>;
	};
};

&i2c4 {
	#address-cells = <1>;
	#size-cells = <0>;

	status = "okay";
	clock-frequency = <400000>;
/*
	sgm41513_chg: charger@1A {
		compatible = "sgm,sgm41513_chg";
		reg = <0x1A>;
		phys = <&hsphy>;
		monitored-battery = <&bat1 &bat2 &bat>;
		extcon = <&extcon_gpio>;
		vddvbus:otg-vbus {
			regulator-name = "vddvbus";
		};
	};
*/	
	sgm4151x_chg: charger@6B {
		compatible = "sgm,sgm4151x_chg";
		reg = <0x6B>;
		phys = <&hsphy>;
		monitored-battery = <&bat1 &bat2 &bat>;
		extcon = <&extcon_gpio>;
		
		vddvbus:otg-vbus {
			regulator-name = "vddvbus";
		};
	};
};
&spi1 {
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";
	ili9882nspi@0{
	compatible = "tl,touchscreen";
	touch,reset-gpio = <&ap_gpio 145 0>;
	touch,irq-gpio = <&ap_gpio 144 0>;
	reg = <0>;
	chipone,rst-gpio = <&ap_gpio 145 0>;
	chipone,irq-gpio = <&ap_gpio 144 0>;
	chipone,x-res = <720>;
	chipone,y-res = <1600>;
	reg_chip_select = <0>;
	
	chipone,test-gesture-rawdata-frames = <1>;
	chipone,gesture-rawdata-min = <1040>;
	chipone,gesture-rawdata-max = <3084>;
	chipone,test-gesture-lp-rawdata-frames = <1>;
	chipone,gesture-lp-rawdata-min = <1040>;
	chipone,gesture-lp-rawdata-max = <3140>;
	chipone,test-gesture-noise-frames = <16>;
	chipone,gesture-noise-max = <145>;
	chipone,test-gesture-lp-noise-frames = <16>;
	chipone,gesture-lp-noise-max = <55>;

	chipone,test-rawdata-frames = <1>;
	chipone,rawdata-min = <1339>;
	chipone,rawdata-max = <3159>;  
	chipone,test-noise-frames = <16>;
	chipone,noise-max = <75>;
	chipone,open-min = <2700>;
	chipone,short-min = <350>;  
	chipone,compensate-cap-min = <1>;
	chipone,compensate-cap-max = <126>;
	
	
	touch,rst-gpio = <&ap_gpio 145 0>;
	/***touch,irq-gpio = <&ap_gpio 144 0>;***/
	focaltech,max-touch-number = <10>;
	focaltech,display-coords =  <0 0 720 1600>;
	
	gcore,rst-gpio = <&ap_gpio 145 0>;
	gcore,irq-gpio = <&ap_gpio 144 0>;
	
	spi-max-frequency = <10000000>;
	};
};
&pmic_fgu {
	monitored-battery = <&bat1 &bat2 &bat>;
	sprd,calib-resistance-micro-ohms = <9886>; /* 10000 */
	sprd,comp-resistance-mohm = <5>;  /* 17 */

	fgu-capacity-track;
	phys = <&hsphy>;
};

&extcon_gpio {
	id-gpio = <&eic_debounce 4 GPIO_ACTIVE_HIGH>;
};

&usb{
	extcon = <&extcon_gpio>;
	status = "okay";
	vbus-supply = <&vddvbus>;
/*	vbus1-supply = <&vddvbus1>;*/
};

&pmic_typec {
	status = "disabled";
};

&i2c5 {
	#address-cells = <1>;
	#size-cells = <0>;
        status = "ok";
	clock-frequency = <400000>;
        st21nfc: st21nfc@08 {
                compatible = "st,st21nfc";
                reg = <0x08>;
                reset-gpios = <&ap_gpio 132 0>;
                irq-gpios = <&ap_gpio 133 0>;
                clkreq-gpios = <&ap_gpio 131 0>;
                status = "ok";
		clock-names = "nfc_ref_clk","source","enable";
		clocks = <&aon_clk CLK_AUX0>, <&ext_26m>, <&aonapb_gate CLK_AUX0_EB>;
        };
};


&pin_controller {
	pinctrl-names =
	/* keyout0 ~ keyout2 */
	"gpio_121", "gpio_122", "gpio_123",
	/* keyin0 ~ keyin2 */
	"gpio_124", "gpio_125", "gpio_126",
	/* extint0, extint1, scl3, sda3 */
	"gpio_144", "gpio_145", "gpio_146", "gpio_147",
	/* Note:If create a new board,need to add the shutdown node */
	"shutdown";
	pinctrl-0 = <&gpio_121>;
	pinctrl-1 = <&gpio_122>;
	pinctrl-2 = <&gpio_123>;
	pinctrl-3 = <&gpio_124>;
	pinctrl-4 = <&gpio_125>;
	pinctrl-5 = <&gpio_126>;
	pinctrl-6 = <&gpio_144>;
	pinctrl-7 = <&gpio_145>;
	pinctrl-8 = <&gpio_146>;
	pinctrl-9 = <&gpio_147>;
	pinctrl-10 = <&shutdown0
			&shutdown1
			&shutdown2
			&shutdown3
			&shutdown4
			&shutdown5>;

	gpio_121: keyout0 {
		pins = "SHARKL3_KEYOUT0";
		function = "func4";
	};

	gpio_122: keyout1 {
		pins = "SHARKL3_KEYOUT1";
		function = "func4";
