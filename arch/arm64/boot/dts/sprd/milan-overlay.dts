/ {
	model = "Spreadtrum SP9863A-1H10-GO Board";

	compatible = "sprd,sp9863a-1h10-go", "sprd,sc9863a";

	sprd,sc-id = "sp9863a milan go";

	fragment {
		target-path = "/";
		__overlay__ {
			#address-cells = <2>;
			#size-cells = <2>;
			dtbo: dtbo {
				compatible = "sprd,dtbo-test";
				reg = <0x0 0x0 0x0 0x11>;
			};
	fingerprint@0 {
				compatible = "chipone,fingerprint";
				fpreset-gpios = <&ap_gpio 156 0>;
				fpint-gpios = <&eic_async 11 0>;
                		ldoven-gpios = <&ap_gpio 73 0>;				
			};
	sensor_switch{
	          compatible = "sprd,sensor_switch";
              sensor_vdd_gpio = <&ap_gpio 53 0>;
            };

			/*** pwm_backlight: sprd_backlight {
				compatible = "pwm-backlight";
				pwms = <&pwms 1 30000>;
				pwm-names = "backlight";
				brightness-levels = <
								  2   2   2   2
					  2   2   2   3   4   5   6   7   8   9
